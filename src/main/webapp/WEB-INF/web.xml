<?xml version="1.0" encoding="UTF-8" ?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>adminLogin</servlet-name>
		<servlet-class>com.admin.adminLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>adminLogin</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>feedbackInsert</servlet-name>
		<servlet-class>com.feedback.feedbackInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>feedbackInsert</servlet-name>
		<url-pattern>/insertfeedback</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updateAdmin</servlet-name>
		<servlet-class>com.admin.updateAdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateAdmin</servlet-name>
		<url-pattern>/updateAdmin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>insertContactus</servlet-name>
		<servlet-class>com.contactus.contactusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertContactus</servlet-name>
		<url-pattern>/insertCon</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>readFeedbacks</servlet-name>
		<servlet-class>com.feedback.feedbackReadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>readFeedbacks</servlet-name>
		<url-pattern>/readFeed</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteFeedbacks</servlet-name>
		<servlet-class>com.feedback.feedbackDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteFeedbacks</servlet-name>
		<url-pattern>/deleteFeed</url-pattern>
	</servlet-mapping>
	
</web-app>